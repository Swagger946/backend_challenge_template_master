		"bcrypt": "^3.0.5",
		"body-parser": "^1.18.3",
		"compression": "^1.7.4",
		"cors": "^2.8.5",
		"dotenv": "^7.0.0",
		"express": "^4.17.1",
		"express-validator": "^5.3.1",
		"express-winston": "^3.1.0",
		"fancy-log": "^1.3.3",
		"helmet": "^3.16.0",
		"jsonwebtoken": "^8.5.1",
		"morgan": "^1.9.1",
		"mysql2": "^1.6.5",
		"nodemon": "^1.18.11",
		"pg-hstore": "^2.3.3",
		"redis": "^2.8.0",
