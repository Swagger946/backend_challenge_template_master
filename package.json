		"babel-jest": "^24.7.1",
		"babel-loader": "^8.0.6",
		"babel-plugin-syntax-dynamic-import": "^6.18.0",
		"chai": "^4.2.0",
		"css-loader": "^3.2.0",
		"eslint": "^5.3.0",
		"eslint-config-airbnb": "^17.1.0",
		"eslint-config-prettier": "^4.1.0",
		"eslint-plugin-import": "^2.16.0",
		"eslint-plugin-jsx-a11y": "^6.2.1",
